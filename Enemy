class Enemy:
        def __init__  (self, name, health, speed, damage):
                self.name = name
                self.health = health
                self.speed  = speed
                self.damage = damage
        def speak(self,sound):
                return f'{self.name} says {sound}'
            
enemy_1 = Enemy("Sylph", 150, 35, 4)
enemy_1.speak("Augh!")
